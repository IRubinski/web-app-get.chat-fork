{
    "name": "getchat-frontend",
    "version": "local",
    "private": true,
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "start": "react-scripts start",
        "local": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:e2e": "cypress run",
        "eject": "react-scripts eject",
        "i18next-conv": "i18next-conv -l en -s src/locales/translation.po src/locales/en/translation.json",
        "prepare": "husky install",
        "lint:staged": "lint-staged --allow-empty",
        "lint:prettier": "prettier --ignore-unknown src",
        "lint:prettier:fix": "prettier --write --ignore-unknown src"
    },
    "lint-staged": {
        "src/**/*": "npm run lint:prettier"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@sentry/react": "^6.19.3",
        "@sentry/tracing": "^6.19.3",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/user-event": "^12.6.0",
        "axios": "^0.21.1",
        "classnames": "^2.3.1",
        "dompurify": "^2.3.3",
        "emoji-mart": "^3.0.0",
        "emoji-regex": "^9.2.1",
        "emoji-tree": "^1.1.2",
        "html-to-text": "^7.0.0",
        "i18next": "^21.6.11",
        "i18next-browser-languagedetector": "^6.1.3",
        "i18next-conv": "^12.0.2",
        "i18next-http-backend": "^1.3.2",
        "i18next-sprintf-postprocessor": "^0.2.2",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "mp3-mediarecorder": "^4.0.3",
        "music-metadata-browser": "^2.2.5",
        "pubsub-js": "^1.9.3",
        "react": "^17.0.2",
        "react-device-detect": "^2.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.15.5",
        "react-moment": "^1.1.1",
        "react-pdf": "^5.2.0",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^5.0.0",
        "source-map-explorer": "^2.5.2",
        "url-search-params-polyfill": "^8.1.1",
        "web-vitals": "^0.2.4",
        "worker-loader": "^3.0.8"
    },
    "//HACK: override `nth-check`": [
        "Override the `nth-check` indirect project dependency, to use its non-vulnerable version",
        "For details, please see: https://gitlab.com/wabbitproject/web-app/-/merge_requests/84#note_933930067"
    ],
    "overrides": {
        "nth-check": "^2.0.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@cypress/react": "^5.12.4",
        "@cypress/webpack-dev-server": "^1.8.4",
        "@material-ui/styles": "^4.11.4",
        "@testing-library/cypress": "^8.0.2",
        "cypress": "^9.5.4",
        "html-webpack-plugin": "^4.5.2",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1"
    },
    "engines": {
        "node": "^16.13.0",
        "npm": "^8.1.0"
    },
    "jest": {
        "moduleNameMapper": {
            "\\./recorder/worker": "<rootDir>/__mocks__/workerMock.js"
        }
    }
}
